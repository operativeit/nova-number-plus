{
    "name": "eom/nova-number-plus",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Eom\\NovaNumberPlus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eom\\NovaNumberPlus\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
